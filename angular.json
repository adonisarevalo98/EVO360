{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "EvaluaSV": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/EvaluaSV",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/plugins/fontawesome-free/css/all.min.css",
              "src/assets/plugins/overlayScrollbars/css/OverlayScrollbars.min.css",
              "src/assets/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css",
              "src/assets/plugins/datatables-responsive/css/responsive.bootstrap4.min.css",
              "src/assets/plugins/datatables-buttons/css/buttons.bootstrap4.min.css",
              "src/assets/plugins/select2/css/select2.min.css",
              "src/assets/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css",
              "src/assets/plugins/toastr/toastr.min.css",
              "src/assets/plugins/dropzone/min/dropzone.min.css",
              "src/assets/dist/css/jquery.datetimepicker.min.css",
              "src/assets/plugins/icheck-bootstrap/icheck-bootstrap.min.css",
              "src/assets/plugins/bootstrap4-toggle/css/bootstrap4-toggle.min.css",
              "src/assets/dist/css/adminlte.min.css",
              "src/assets/dist/css/styles.css",
              "src/assets/plugins/summernote/summernote-bs4.min.css",
              "src/assets/MDB5/css/tables.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.min.css"
            ],
            "scripts": [
              "src/assets/plugins/jquery/jquery.min.js",
              "src/assets/plugins/jquery-ui/jquery-ui.min.js",
              "src/assets/plugins/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js",
              "src/assets/dist/js/adminlte.js",
              "src/assets/plugins/jquery-mousewheel/jquery.mousewheel.js",
              "src/assets/plugins/raphael/raphael.min.js",
              "src/assets/plugins/jquery-mapael/jquery.mapael.min.js",
              "src/assets/plugins/jquery-mapael/maps/usa_states.min.js",
              "src/assets/plugins/chart.js/Chart.min.js",
              "src/assets/dist/js/demo.js",
              "src/assets/dist/js/pages/dashboard2.js",
              "src/assets/plugins/datatables/jquery.dataTables.min.js",
              "src/assets/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js",
              "src/assets/plugins/datatables-responsive/js/dataTables.responsive.min.js",
              "src/assets/plugins/datatables-responsive/js/responsive.bootstrap4.min.js",
              "src/assets/plugins/datatables-buttons/js/dataTables.buttons.min.js",
              "src/assets/plugins/datatables-buttons/js/buttons.bootstrap4.min.js",
              "src/assets/plugins/jszip/jszip.min.js",
              "src/assets/plugins/pdfmake/pdfmake.min.js",
              "src/assets/plugins/pdfmake/vfs_fonts.js",
              "src/assets/plugins/datatables-buttons/js/buttons.html5.min.js",
              "src/assets/plugins/datatables-buttons/js/buttons.print.min.js",
              "src/assets/plugins/datatables-buttons/js/buttons.colVis.min.js",
              "src/assets/plugins/bootstrap4-toggle/js/bootstrap4-toggle.min.js",
              "src/assets/plugins/select2/js/select2.full.min.js",
              "src/assets/plugins/summernote/summernote-bs4.min.js",
              "src/assets/plugins/dropzone/min/dropzone.min.js",
              "src/assets/plugins/bs-custom-file-input/bs-custom-file-input.min.js",
              "src/assets/dist/js/jquery.datetimepicker.full.min.js",
              "src/assets/plugins/bootstrap-switch/js/bootstrap-switch.min.js",
              "src/assets/plugins/moment/moment.min.js",
              "src/assets/MDB5/js/tables.js",


              
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/datatables.net/js/jquery.dataTables.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "EvaluaSV:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "EvaluaSV:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "EvaluaSV:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "EvaluaSV:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "EvaluaSV:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "EvaluaSV"
}